{
  "name": "biblys/payplug-php",
  "description": "A fork of the PHP library for PayPlug public API.",
  "version": "2.2.1",
  "type": "library",
  "require": {
    "php": ">=5.3.0",
    "ext-openssl": "*",
    "ext-curl": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "4.6.*",
    "phpdocumentor/phpdocumentor": "2.*"
  },
  "minimum-stability": "stable",
  "autoload": {
    "psr-0": {"Payplug\\": "lib/"},
    "psr-4": {"Payplug\\": "lib/Payplug"}
  }
}
